{
  "moduleFileExtensions": [
    "js", "ts"
  ],
  "transform": {
    "\\.ts$": "ts-jest"
  },
  "globals": {
    "ts-jest": {
      "tsConfig": "tsconfig.json"
    }
  },
  "testMatch": [
    "**/?(*.)spec.ts"
  ],
  "testPathIgnorePatterns": [
    "./node_modules",
    "./dist"
  ],
  "setupFiles": [
    "./fixtures/env.ts"
  ],
  "testEnvironment": "node"
}
